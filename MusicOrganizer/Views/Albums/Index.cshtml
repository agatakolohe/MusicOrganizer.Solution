@using MusicOrganizer.Models;

@{
    Layout = "_Layout";
}

<h1>Albums</h1>

@if (@Model.Count == 0)
{
    <h3>No albums have been added yet!</h3>
}

@foreach (Album album in Model)
{
    <h3><a href='/albums/@album.Id'>@album.AlbumName</a></h3>
}

<p><a href='/'>Back home</a></p>
